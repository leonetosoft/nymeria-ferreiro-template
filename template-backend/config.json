        "sequelize-typescript": [
            "Table",
            "Model",
            "PrimaryKey",
            "Column",
            "DataType",
            "AutoIncrement",
            "Default",
            "Comment",
            "ForeignKey",
            "BelongsTo"
        ],
        "../util/primeng/CrudUtils": [
